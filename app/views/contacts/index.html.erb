<header>
	<h1>Contacts</h1>
	<ul class="actions">
		<li><%= link_to "New Contact", new_contact_path, class: "new" %></li>
	</ul>
</header>

<div id="contacts">
	<% @contacts.each do |contact| %>	
	<h2><%= link_to contact.full_name, contact %></h2>
	<p><%= contact.cell_phone %></p>
	<p><%= contact.email %></p>
	<p><%= contact.spouse %></p>
	<p><%= contact.children %></p>
	<p><%= contact.company %></p>
	<p><%= contact.description %></p>
	<p><%= contact.first_met %></p>
	<% end %>
</div>


