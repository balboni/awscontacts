<div class="page-header">
	<h1>Contacts</h1>
	<ul class="actions">
		<li><%= link_to fa_icon("plus") + "New Contact", new_contact_path,
		  class: "btn btn-success" %></li>
	</ul>
</div>

<div id="contacts">
	<% @contacts.each do |contact| %>
	<h2><%= contact.fname %></h2>
	<h2><%= link_to contact.lname, contact %></h2>
	<p><%= contact.cell_phone %></p>
	<p><%= contact.email %></p>
	<p><%= contact.company %></p>
	<p><%= contact.description %></p>
	<p><%= contact.first_met %></p>
	<% end %>
</div>


