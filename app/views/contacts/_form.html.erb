<%= form_for(contact) do |f| %>

<% if contact.errors.any? %>
  <div id="error_explanation">
  	<h2><%= pluralize(contact.errors.count, "error") %>
  		prohibited this project from being saved: </h2>

  		<ul>
  			<% contact.errors.full_messages.each do |msg| %>
  			<li><%= msg %></li>
  			<% end %>
  		</ul>
  	</div>
  	<% end %>
	<p>
		<%= f.label :fname, "First Name" %><br>
		<%= f.text_field :fname %><br>
		<%= f.label :lname, "Last Name" %><br>
		<%= f.text_field :lname %><br>
	</p>

	<p>
		<%= f.label :cell_phone, "Cell Phone" %><br>
		<%= f.text_field :cell_phone %><br>
		<%= f.label :email %><br>
		<%= f.text_field :email %><br>
		<%= f.label :company %><br>
		<%= f.text_field :company %><br>
		<%= f.label :first_met, "First Met" %><br>
		<%= f.text_field :first_met %><br>
	</p>

	<p>
		<%= f.label :description %><br>
		<%= f.text_area :description %><br>
	</p>

	<%= f.submit %>
<% end %>