<%= simple_form_for(contact) do |f| %>

		<%= f.input :fname, :label => "First Name" %>
		<%= f.input :lname, :label => "Last Name" %>
		<%= f.input :cell_phone %>
		<%= f.input :email %>
		<%= f.input :company %>
		<%= f.input :first_met %>
		<%= f.input :description %>

	<%= f.button :submit, class: "btn-primary" %>
<% end %>